<!DOCTYPE html>
<html>

<!-- SlideShow Scripts -->
<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.0/umd/popper.min.js" integrity="sha384-cs/chFZiN24E4KMATLdqdvsezGxaGsi4hLGOzlXwp5UZB1LY//20VyM2taTB4QvJ" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.0/js/bootstrap.min.js" integrity="sha384-uefMccjFJAIv6A+rW+L4AHf99KvxDjWSu1z9VI8SKNVmz4sk7buKt/6v9KI65qnm" crossorigin="anonymous"></script>

<!-- Navigation Bar -->
<div class="text-center">
  <div class="cover-container d-flex w-100 h-100 p-3 mx-auto flex-column">

    <header class="masthead mb-auto">
      <div class="inner">

        <!-- NavItems -->
        <nav class="navbar">

          <!-- Logo Brand -->
          <%= image_tag("RootLogo.png", :alt => "RootLogo", :class => "RootlLogo") %>


          <div class="nav-item">

            <!-- Link Tags -->
            <%= link_to "Home", root_path %>
            <%= link_to "Explore", explore_path %>
            <%= link_to "About", about_path %>

          </div>



          <!-- User Icons & Profiles -->
          <% if @current_user %>

            <div>

              <!-- Wishlist Icon -->
              <%= link_to profile_path, :class => 'user-icons', :style => 'text-decoration: none;' do %>
                <span class="badge badge-primary badge-pill">32</span>
                <svg width="1.8em" height="1.8em" viewBox="0 0 16 16" class="bi bi-heart-fill icon-hover" fill="white" xmlns="http://www.w3.org/2000/svg">
                  <path fill-rule="evenodd" d="M8 1.314C12.438-3.248 23.534 4.735 8 15-7.534 4.736 3.562-3.248 8 1.314z"/>
                </svg>
              <% end %>




              <!-- Cart Icon -->
              <%= link_to profile_path, :class => 'user-icons', :style => 'text-decoration: none;' do %>
                <span class="badge badge-primary badge-pill">6</span>
                <svg width="1.8em" height="1.8em" viewBox="0 0 16 16" class="bi bi-bag-fill user-icons icon-hover" fill="white" xmlns="http://www.w3.org/2000/svg">
                  <path fill-rule="evenodd" d="M8 1a2.5 2.5 0 0 0-2.5 2.5V4h5v-.5A2.5 2.5 0 0 0 8 1zm3.5 3v-.5a3.5 3.5 0 1 0-7 0V4H1v10a2 2 0 0 0 2 2h10a2 2 0 0 0 2-2V4h-3.5z"/>
                </svg>
              <% end %>

              <!-- Profile Icon -->
              <%= link_to profile_path, :class => 'user-icons', :style => 'text-decoration: none;' do %>
                <svg width="1.8em" height="1.8em" viewBox="0 0 16 16" class="bi bi-person-circle user-icons icon-hover" fill="white" xmlns="http://www.w3.org/2000/svg">
                  <path d="M13.468 12.37C12.758 11.226 11.195 10 8 10s-4.757 1.225-5.468 2.37A6.987 6.987 0 0 0 8 15a6.987 6.987 0 0 0 5.468-2.63z"/>
                  <path fill-rule="evenodd" d="M8 9a3 3 0 1 0 0-6 3 3 0 0 0 0 6z"/>
                  <path fill-rule="evenodd" d="M8 1a7 7 0 1 0 0 14A7 7 0 0 0 8 1zM0 8a8 8 0 1 1 16 0A8 8 0 0 1 0 8z"/>
                </svg>
              <% end %>

            </div>


            <!-- User Login & Signup Buttons -->
            <% else %>

            <div class="nav-buttons">
              <%= link_to "LogIn", login_path, :class => 'btn btn-primary login-button' %>
              <%= link_to "SignUp", signup_path, :class => 'btn btn-primary signup-button' %>
            </div>

        <% end %>


        </nav>

      </div>
    </header>

  </div>

</div>





<!-- Body Contents -->
<body style="background-color: #152734; overflow-x: hidden;">




<!-- Main Head -->
<main class="mx-auto inner flex-column">
  <div class="main img-fluid">

    <!-- Main Image -->
    <%= image_tag("RootImage.png", :alt => 'RootImage', :class => "RootImage float-right img-fluid") %>


    <!-- Main Content -->
    <h1 class="main-content">Learning <br> From Home</h1>

  </div>
</main>

<br> <br> <br> <br> <br> <br> <br> <br> <br> <br> <br> <br> <br> <br> <br> <br> <br> <br>


<!-- Slide Show -->
<div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">

  <!-- Target Links -->
  <ol class="carousel-indicators">
    <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
    <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
    <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
  </ol>

  <!-- Image Block -->
  <div class="carousel-inner">

    <!-- Image One -->
    <div class="carousel-item active" id="slide-one">
      <%= image_tag("rec-one.png", class: "d-block w-100 lazy", width: "1100", height: "800") %>
    </div>

    <!-- Image Two -->
    <div class="carousel-item" id="slide-two">
      <%= image_tag("rec-four.png", class: "d-block w-100 lazy", width: "1100", height: "800") %>
    </div>

    <!-- Image Three -->
    <div class="carousel-item" id="slide-three">
      <%= image_tag("rec-six.png", class: "d-block w-100 lazy", width: "1100", height: "800") %>
    </div>

  </div>

  <!-- Next Button -->
  <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>

  <!-- Previous Button -->
  <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>

</div>

<br> <br> <br>


