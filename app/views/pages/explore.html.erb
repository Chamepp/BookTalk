<!-- Administration Card -->
<div class="card-deck d-flex justify-content-center">

  <!-- Split Start -->

    <% @card.each do |card| %>


      <!-- Admin Card -->
      <div class="card card-deck-item shadow-sm">

        <!-- Header Image -->
        <% if card.cardpic.attached? %>
          <%= image_tag(url_for(card.cardpic), :alt => 'Card image cap', :class => 'card-img-top') %>
        <% end %>

        <!-- Card Body -->
        <div class="card-body">
          <h4 class="card-title"> <%= card.cardname %> </h4>
          <h5 class="card-title">$ <%= card.cardprice %></h5>
          <p class="card-text"> <%= card.carddes %> </p>
          <%= link_to card_path(card), :class => 'btn btn-primary' do %>
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-eye-fill" viewBox="0 0 16 16">
              <path d="M10.5 8a2.5 2.5 0 1 1-5 0 2.5 2.5 0 0 1 5 0z"/>
              <path fill-rule="evenodd" d="M0 8s3-5.5 8-5.5S16 8 16 8s-3 5.5-8 5.5S0 8 0 8zm8 3.5a3.5 3.5 0 1 0 0-7 3.5 3.5 0 0 0 0 7z"/>
            </svg>
            Preview
          <% end %>

          <a href="#" class="btn btn-danger">
            <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-heart-fill" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
              <path fill-rule="evenodd" d="M8 1.314C12.438-3.248 23.534 4.735 8 15-7.534 4.736 3.562-3.248 8 1.314z"/>
            </svg>
          </a>
          <% if @current_admin %>
            <!-- Edit Button -->
            <%= link_to edit_card_path(card), :class => 'btn btn-primary edit-button' do %>
            <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-pen-fill" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
              <path fill-rule="evenodd" d="M13.498.795l.149-.149a1.207 1.207 0 1 1 1.707 1.708l-.149.148a1.5 1.5 0 0 1-.059 2.059L4.854 14.854a.5.5 0 0 1-.233.131l-4 1a.5.5 0 0 1-.606-.606l1-4a.5.5 0 0 1 .131-.232l9.642-9.642a.5.5 0 0 0-.642.056L6.854 4.854a.5.5 0 1 1-.708-.708L9.44.854A1.5 1.5 0 0 1 11.5.796a1.5 1.5 0 0 1 1.998-.001z"/>
            </svg>
            <% end %>

            <!-- Delete Button -->
            <%= link_to card_path(card), method: :delete, :class => 'btn btn-primary delete-button' do %>
              <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-trash-fill" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                <path fill-rule="evenodd" d="M2.5 1a1 1 0 0 0-1 1v1a1 1 0 0 0 1 1H3v9a2 2 0 0 0 2 2h6a2 2 0 0 0 2-2V4h.5a1 1 0 0 0 1-1V2a1 1 0 0 0-1-1H10a1 1 0 0 0-1-1H7a1 1 0 0 0-1 1H2.5zm3 4a.5.5 0 0 1 .5.5v7a.5.5 0 0 1-1 0v-7a.5.5 0 0 1 .5-.5zM8 5a.5.5 0 0 1 .5.5v7a.5.5 0 0 1-1 0v-7A.5.5 0 0 1 8 5zm3 .5a.5.5 0 0 0-1 0v7a.5.5 0 0 0 1 0v-7z"/>
              </svg>
            <% end %>
          <% end %>
        </div>

        <!-- Card Footer -->
        <div class="card-footer">
          <% if card.cardpremium.eql?("true") %>
            <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-gem" fill="#08fcac" xmlns="http://www.w3.org/2000/svg">
              <path fill-rule="evenodd" d="M3.1.7a.5.5 0 0 1 .4-.2h9a.5.5 0 0 1 .4.2l2.976 3.974c.149.185.156.45.01.644L8.4 15.3a.5.5 0 0 1-.8 0L.1 5.3a.5.5 0 0 1 0-.6l3-4zm11.386 3.785l-1.806-2.41-.776 2.413 2.582-.003zm-3.633.004l.961-2.989H4.186l.963 2.995 5.704-.006zM5.47 5.495l5.062-.005L8 13.366 5.47 5.495zm-1.371-.999l-.78-2.422-1.818 2.425 2.598-.003zM1.499 5.5l2.92-.003 2.193 6.82L1.5 5.5zm7.889 6.817l2.194-6.828 2.929-.003-5.123 6.831z"/>
            </svg>
            Premium

          <% else %>
            <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-dot" fill="white" xmlns="http://www.w3.org/2000/svg">
              <path fill-rule="evenodd" d="M8 9.5a1.5 1.5 0 1 0 0-3 1.5 1.5 0 0 0 0 3z"/>
            </svg>
            Free
          <% end %>
        </div>

      </div>

    <% end %>

</div>




