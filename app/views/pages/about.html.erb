<html>


<!-- Navigation Bar -->
<div class="text-center">
  <div class="cover-container d-flex w-100 h-100 p-3 mx-auto flex-column">

    <header class="masthead mb-auto">
      <div class="inner">

        <!-- NavItems -->
        <nav class="navbar">

          <!-- Logo Brand -->
          <%= image_tag("RootLogo.png", :alt => "RootLogo", :class => "RootlLogo") %>


          <div class="nav-item">

            <!-- Link Tags -->
            <%= link_to "Home", root_path %>
            <%= link_to "Explore", explore_path %>
            <%= link_to "About", about_path %>

          </div>




          <!-- SignUp & Login Buttons -->
          <% if @user == false %>
            <div class="nav-buttons">
              <%= link_to "LogIn", login_path, :class => 'btn btn-primary login-button' %>
              <%= link_to "SignUp", signup_path, :class => 'btn btn-primary signup-button' %>
            </div>


            <!-- User Controls & Profiles -->
          <% else %>

            <div>

              <!-- Wishlist Icon -->
              <%= link_to explore_path, :class => 'user-icons', :style => 'text-decoration: none;' do %>
                <span class="badge badge-primary badge-pill">32</span>
                <svg width="1.8em" height="1.8em" viewBox="0 0 16 16" class="bi bi-heart-fill icon-hover" fill="white" xmlns="http://www.w3.org/2000/svg">
                  <path fill-rule="evenodd" d="M8 1.314C12.438-3.248 23.534 4.735 8 15-7.534 4.736 3.562-3.248 8 1.314z"/>
                </svg>
              <% end %>




              <!-- Cart Icon -->
              <%= link_to explore_path, :class => 'user-icons', :style => 'text-decoration: none;' do %>
                <span class="badge badge-primary badge-pill">6</span>
                <svg width="1.8em" height="1.8em" viewBox="0 0 16 16" class="bi bi-bag-fill user-icons icon-hover" fill="white" xmlns="http://www.w3.org/2000/svg">
                  <path fill-rule="evenodd" d="M8 1a2.5 2.5 0 0 0-2.5 2.5V4h5v-.5A2.5 2.5 0 0 0 8 1zm3.5 3v-.5a3.5 3.5 0 1 0-7 0V4H1v10a2 2 0 0 0 2 2h10a2 2 0 0 0 2-2V4h-3.5z"/>
                </svg>
              <% end %>

              <!-- Profile Icon -->
              <%= link_to explore_path, :class => 'user-icons', :style => 'text-decoration: none;' do %>
                <svg width="1.8em" height="1.8em" viewBox="0 0 16 16" class="bi bi-person-circle user-icons icon-hover" fill="white" xmlns="http://www.w3.org/2000/svg">
                  <path d="M13.468 12.37C12.758 11.226 11.195 10 8 10s-4.757 1.225-5.468 2.37A6.987 6.987 0 0 0 8 15a6.987 6.987 0 0 0 5.468-2.63z"/>
                  <path fill-rule="evenodd" d="M8 9a3 3 0 1 0 0-6 3 3 0 0 0 0 6z"/>
                  <path fill-rule="evenodd" d="M8 1a7 7 0 1 0 0 14A7 7 0 0 0 8 1zM0 8a8 8 0 1 1 16 0A8 8 0 0 1 0 8z"/>
                </svg>
              <% end %>

            </div>

          <% end %>


        </nav>

      </div>
    </header>

  </div>

</div>


